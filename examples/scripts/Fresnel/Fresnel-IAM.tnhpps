print("azimuth [deg], elevation[deg], efficiency");
var azimuth = 90;
for (var elevation = 10; elevation <= 90; elevation += 10) 
{
	var sp = NodeObject().getScene().getPart("world.sun.position");
	sp.setParameter("azimuth", azimuth);
	sp.setParameter("elevation", elevation);
	
	var w = tn.FindInterception("//Node/Fresnel_CSP/Receiver/Absorber/Shape", 1e6);
	var wMax = 18*8*0.32*4*1000; // rows*mirrors*width*length*DNI
	var eta = w/wMax;

	print("" + azimuth + ", " + elevation + ", " + eta);
}